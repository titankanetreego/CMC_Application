<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/background_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@mipmap/ic_u2_hmi_1"
        android:visibility="gone">

        <ImageView
            android:id="@+id/backdoor_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="693dp"
            android:layout_marginTop="60dp"
            android:src="@mipmap/u2_hmi_logo"
            android:layout_marginStart="693dp" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/engineering_mode_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/gs_d"
        android:orientation="horizontal"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="5dp">

            <EditText
                android:id="@+id/set_recharging_time_edit"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:imeOptions="actionDone"
                android:inputType="text"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/set_recharging_time_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:autoText="true"
                android:background="@drawable/en_mode_button_selector"
                android:text="@string/set_recharging_time"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <EditText
                android:id="@+id/set_max_voltage_edit"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:imeOptions="actionDone"
                android:inputType="text"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/set_max_voltage_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@drawable/en_mode_button_selector"
                android:text="@string/set_max_voltage"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <EditText
                android:id="@+id/set_max_current_edit"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:imeOptions="actionDone"
                android:inputType="text"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/set_max_current_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@drawable/en_mode_button_selector"
                android:text="@string/set_max_current"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal"
                android:layout_marginTop="5dp">

                <EditText
                    android:id="@+id/set_years_edit"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/gs_7"
                    android:gravity="center"
                    android:imeOptions="actionDone"
                    android:maxLength="2"
                    android:inputType="number"
                    android:textColor="@color/white"
                    android:textSize="24sp"
                    android:theme="@style/EditTextTheme"
                    tools:ignore="LabelFor" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/white" />

                <EditText
                    android:id="@+id/set_months_edit"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/gs_7"
                    android:gravity="center"
                    android:imeOptions="actionDone"
                    android:maxLength="2"
                    android:inputType="number"
                    android:textColor="@color/white"
                    android:textSize="24sp"
                    android:theme="@style/EditTextTheme"
                    tools:ignore="LabelFor" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/white" />

                <EditText
                    android:id="@+id/set_serial_edit"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/gs_7"
                    android:gravity="center"
                    android:imeOptions="actionDone"
                    android:inputType="number"
                    android:textColor="@color/white"
                    android:textSize="24sp"
                    android:theme="@style/EditTextTheme"
                    tools:ignore="LabelFor" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/set_years_button"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/en_mode_button_selector"
                    android:text="@string/set_years_button"
                    android:textColor="@color/white"
                    android:textSize="24sp" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/gs_d" />

                <Button
                    android:id="@+id/set_months_button"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/en_mode_button_selector"
                    android:text="@string/set_months_button"
                    android:textColor="@color/white"
                    android:textSize="24sp" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/gs_d" />

                <Button
                    android:id="@+id/set_serial_button"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/en_mode_button_selector"
                    android:text="@string/set_serial_button"
                    android:textColor="@color/white"
                    android:textSize="24sp" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="5dp">

            <TextView
                android:id="@+id/voltage_first_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/voltage_first_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_4"
                android:text="@string/set_power_voltage"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/voltage_second_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/voltage_second_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_4"
                android:text="@string/set_charging_voltage"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/recharging_current_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/recharging_current_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_4"
                android:text="@string/set_charging_current"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/kiosk_tv"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="70"
                    android:background="@color/gs_7"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textSize="24sp"
                    android:theme="@style/EditTextTheme"
                    tools:ignore="LabelFor" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="30"
                    android:background="@color/gs_9"
                    android:gravity="center|start"
                    android:paddingLeft="5dp"
                    android:text="@string/text_control_mode"
                    android:textColor="@color/black"
                    android:textSize="16sp" />

            </LinearLayout>

            <Button
                android:id="@+id/set_control_mode_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_4"
                android:text="@string/set_control_mode"
                android:textColor="@color/white"
                android:textSize="24sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="5dp">

            <Button
                android:id="@+id/callback_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@drawable/en_mode_button_selector"
                android:text="@string/callback"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/secc_ver_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <TextView
                android:id="@+id/soc_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/soc_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_4"
                android:text="@string/set_soc"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/insulation_detection_p_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/insulation_detection_p_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_4"
                android:text="@string/set_insulation_detection_p"
                android:textColor="@color/white"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/insulation_detection_n_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/insulation_detection_n_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_4"
                android:text="@string/set_insulation_detection_n"
                android:textColor="@color/white"
                android:textSize="22sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp">

            <TextView
                android:id="@+id/status_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/status_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@drawable/en_mode_button_selector"
                android:text="@string/set_work_status"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/secc_error_code_tv"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="5dp"
                android:background="@color/gs_7"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="24sp"
                android:theme="@style/EditTextTheme"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/secc_error_code_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@drawable/en_mode_button_selector"
                android:text="@string/set_error_code"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal"
                android:layout_marginTop="5dp">

                <TextView
                    android:id="@+id/module_code_l_tv"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/gs_7"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textSize="24sp"
                    android:theme="@style/EditTextTheme"
                    tools:ignore="LabelFor" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"/>

                <TextView
                    android:id="@+id/module_code_h_tv"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/gs_7"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textSize="24sp"
                    android:theme="@style/EditTextTheme"
                    tools:ignore="LabelFor" />

            </LinearLayout>

            <Button
                android:id="@+id/set_module_error_code_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@drawable/en_mode_button_selector"
                android:text="@string/set_module_error_code"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal"
                android:layout_marginTop="5dp">

                <TextView
                    android:id="@+id/evcc_l_tv"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/gs_7"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textSize="24sp"
                    android:theme="@style/EditTextTheme"
                    tools:ignore="LabelFor" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"/>

                <TextView
                    android:id="@+id/evcc_h_tv"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@color/gs_7"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textSize="24sp"
                    android:theme="@style/EditTextTheme"
                    tools:ignore="LabelFor" />

            </LinearLayout>

            <Button
                android:id="@+id/set_car_status_button"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@drawable/en_mode_button_selector"
                android:text="@string/set_car_status"
                android:textColor="@color/white"
                android:textSize="24sp" />

        </LinearLayout>

    </LinearLayout>


</LinearLayout>